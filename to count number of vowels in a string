#include <stdio.h>

int main()
{
    int i,count=0;
    char array[13] = {'p','r','e','e','t','h','i','s','e','d','i','g','a'};
    for(i=0;i<=13;i++)
    {
        if(array[i]=='a'|| array[i]=='e'|| array[i]=='i'|| array[i]=='o'|| array[i]=='u')
        {
            count++;
            printf("%c\t",array[i]);
        }
    }
    printf("\n%d",count);
}
